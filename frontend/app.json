{
  "expo": {
    "name": "Kweek",
    "plugins": [
      [
        "./plugin/withDisableForcedDarkModeAndroid.js",
        {}
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to allow you to upload them for your own marketing purposes."
        }
      ]
    ],
    "slug": "Kweek",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "userInterfaceStyle": "light"
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "kweek.tp.com",
      "versionCode": 2,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/kweekFav.png"
    },
    "extra": {
      "eas": {
        "projectId": "25fbcb6a-5dfe-43fc-8db6-a3d3eae14677"
      }
    }
  }
}
